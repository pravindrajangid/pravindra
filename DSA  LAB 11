// Program 11: Queue using circular array
#include <iostream>
using namespace std;
const int MAXNQ = 100;

int main(){
    int q[MAXNQ], front=0, rear=0, size=0, choice, val;
    while(true){
        cout<<"\n1.Enqueue 2.Dequeue 3.Peek 4.Display 5.Exit\nChoice: "; cin>>choice;
        if(choice==1){
            if(size==MAXNQ) cout<<"Overflow\n";
            else { cout<<"Value: "; cin>>val; q[rear]=val; rear=(rear+1)%MAXNQ; size++; }
        } else if(choice==2){
            if(size==0) cout<<"Underflow\n";
            else { cout<<"Dequeued "<<q[front]<<"\n"; front=(front+1)%MAXNQ; size--; }
        } else if(choice==3){
            if(size==0) cout<<"Empty\n"; else cout<<"Front = "<<q[front]<<"\n";
        } else if(choice==4){
            if(size==0) cout<<"Empty\n"; else { cout<<"Queue: ";
                for(int i=0, idx=front;i<size;i++, idx=(idx+1)%MAXNQ) cout<<q[idx]<<" ";
                cout<<"\n";
            }
        } else break;
    }
    return 0;
}
